import * as React from 'react';
import ReactDOM from 'react-dom';
import App from './components/App';
import type { AppProperties } from './components/App/App';

export default (initialState: AppProperties, el: HTMLElement) => {
  {{#if widget}}
    ReactDOM.render(
  {{else}}
    ReactDOM.hydrate(
  {{/if}}
    <App {...initialState} />, el);
};
